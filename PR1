#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <math.h>

// Задача 1: Напишите программу, которая принимает от пользователя число и выводит его на экран.
/*
#include <stdio.h>
int main() {
    int number;
    printf("Введите число: ");
    scanf("%d", &number);
    printf("Вы ввели: %d\n", number);
    return 0;
}
*/

// Задача 2: Напишите программу, которая принимает от пользователя два числа и выводит на экран их сумму, разность, произведение и частное.
/*
#include <stdio.h>
int main() {
    int a, b;
    printf("Введите первое число: ");
    scanf("%d", &a);
    printf("Введите второе число: ");
    scanf("%d", &b);
    
    printf("Сумма: %d + %d = %d\n", a, b, a + b);
    printf("Разность: %d - %d = %d\n", a, b, a - b);
    printf("Произведение: %d * %d = %d\n", a, b, a * b);
    if (b != 0) {
        printf("Частное: %d / %d = %.2f\n", a, b, (double)a / b);
    } else {
        printf("Деление на ноль невозможно!\n");
    }
    return 0;
}
*/

// Задача 3: Напишите программу, которая принимает от пользователя три числа и выводит на экран их среднее арифметическое.
/*
#include <stdio.h>
int main() {
    int a, b, c;
    double average;
    printf("Введите три числа: ");
    scanf("%d %d %d", &a, &b, &c);
    average = (a + b + c) / 3.0;
    printf("Среднее арифметическое: %.2f\n", average);
    return 0;
}
*/

// Задача 4: Напишите программу, которая принимает от пользователя его имя и выводит на экран приветствие.
/*
#include <stdio.h>
int main() {
    char name[50];
    printf("Введите ваше имя: ");
    scanf("%s", name);
    printf("Привет, %s!\n", name);
    return 0;
}
*/

// Задача 5: Напишите программу, которая принимает от пользователя его год рождения и выводит на экран его возраст.
/*
#include <stdio.h>
int main() {
    int birth_year, current_year = 2023; // Предположим, что текущий год 2023
    printf("Введите ваш год рождения: ");
    scanf("%d", &birth_year);
    printf("Ваш возраст: %d лет\n", current_year - birth_year);
    return 0;
}
*/

// Задача 7: Напишите программу, которая принимает от пользователя число и выводит на экран, является ли оно четным или нечетным.
/*
#include <stdio.h>
int main() {
    int number;
    printf("Введите число: ");
    scanf("%d", &number);
    if (number % 2 == 0) {
        printf("Число %d - четное.\n", number);
    } else {
        printf("Число %d - нечетное.\n", number);
    }
    return 0;
}
*/

// Задача 8: Напишите программу, которая принимает от пользователя число и выводит на экран, является ли оно положительным или отрицательным.
/*
#include <stdio.h>
int main() {
    int number;
    printf("Введите число: ");
    scanf("%d", &number);
    if (number > 0) {
        printf("Число %d - положительное.\n", number);
    } else if (number < 0) {
        printf("Число %d - отрицательное.\n", number);
    } else {
        printf("Число %d - ноль.\n", number);
    }
    return 0;
}
*/

// Задача 9: Напишите программу, которая принимает от пользователя число и выводит на экран, является ли оно кратным 3.
/*
#include <stdio.h>
int main() {
    int number;
    printf("Введите число: ");
    scanf("%d", &number);
    if (number % 3 == 0) {
        printf("Число %d кратно 3.\n", number);
    } else {
        printf("Число %d не кратно 3.\n", number);
    }
    return 0;
}
*/

// Задача 10: Напишите программу, которая принимает от пользователя два числа и выводит на экран большее из них.
/*
#include <stdio.h>
int main() {
    int a, b;
    printf("Введите два числа: ");
    scanf("%d %d", &a, &b);
    if (a > b) {
        printf("Большее число: %d\n", a);
    } else if (b > a) {
        printf("Большее число: %d\n", b);
    } else {
        printf("Оба числа равны: %d\n", a);
    }
    return 0;
}
*/

// Задача 11: Напишите программу, которая принимает от пользователя два числа и выводит на экран, равны ли они.
/*
#include <stdio.h>
int main() {
    int a, b;
    printf("Введите два числа: ");
    scanf("%d %d", &a, &b);
    if (a == b) {
        printf("Числа равны.\n");
    } else {
        printf("Числа не равны.\n");
    }
    return 0;
}
*/

// Задача 12 (циклы): Напишите программу, которая выводит на экран числа от 1 до 10.
/*
#include <stdio.h>
int main() {
    int i;
    for (i = 1; i <= 10; i++) {
        printf("%d ", i);
    }
    printf("\n");
    return 0;
}
*/

// Задача 13 (циклы): Напишите программу, которая выводит на экран таблицу умножения на 5.
/*
#include <stdio.h>
int main() {
    int i;
    printf("Таблица умножения на 5:\n");
    for (i = 1; i <= 10; i++) {
        printf("5 * %d = %d\n", i, 5 * i);
    }
    return 0;
}
*/

// Задача 14 (циклы): Напишите программу, которая выводит на экран сумму чисел от 1 до 100.
/*
